# If no conditions are given, *all* conditionals are expanded. We don't want
# to include any conditions by default, so we need to pass a phony conditional
# conditionals are delimeted with a semicolon
SUDO_CONDS = ;with_sudo
AUTOFS_CONDS = ;with_autofs
SSH_CONDS = ;with_ssh
PAC_RESPONDER_CONDS = ;with_pac_responder
IFP_CONDS = ;with_ifp
#SEC_CONDS = ;with_secrets
KCM_CONDS = ;with_kcm
#STAP_CONDS = ;with_stap
#GPO_CONDS = ;gpo_default_permissive
GPO_CONDS = ;gpo_default_enforcing
#SYSTEMD_CONDS = ;have_systemd
FILES_CONDS = ;no_enable_files_domain
#FILES_CONDS = ;enable_files_domain
#CRYPTO_CONDS = ;with_openssl
CRYPTO_CONDS = ;with_nss
#LOCAL_PROVIDER_CONDS = ;enable_local_provider
CONDS = with_false$(SUDO_CONDS)$(AUTOFS_CONDS)$(SSH_CONDS)$(PAC_RESPONDER_CONDS)$(IFP_CONDS)$(GPO_CONDS)$(SEC_CONDS)$(SYSTEMD_CONDS)$(FILES_CONDS)$(KCM_CONDS)$(STAP_CONDS)$(CRYPTO_CONDS)$(LOCAL_PROVIDER_CONDS)
